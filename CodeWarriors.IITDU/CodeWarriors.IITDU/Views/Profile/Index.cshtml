@model CodeWarriors.IITDU.ViewModels.ProfileModel

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="col-md-6 top-bottom-padding">
    <h3>Profile</h3>
    <table class="table table-hover table-bordered table-striped">
        <tr>
            <td>Name</td>
            <td>@Model.FirstName @Model.LastName</td>
        </tr>
        <tr>
            <td>Gender</td>
            <td>@Model.Gender</td>
        </tr>
        <tr>
            <td>Email</td>
            <td>@Model.Email</td>
        </tr>
        <tr>
            <td>Age</td>
            <td>@Model.Age</td>
        </tr>
        <tr>
            <td>Country</td>
            <td>@Model.Country</td>
        </tr>
        <tr>
            <td>About</td>
            <td>@Model.About</td>
        </tr>
    </table>
    <a href="/Profile/Edit" class="btn btn-info">Edit</a>
</div>

    
